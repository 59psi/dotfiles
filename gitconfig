[alias]
	ap = add --patch
	au = add --update
	br = branch
	bv = branch --verbose --verbose
	ca = commit --amend
	cb = checkout -b
	cf = cat-file
	ci = commit
	co = checkout
	cp = cherry-pick
	cs = commit --signoff
	cv = commit --verbose
	cw = commit -am "wip"
	dc = diff --cached
	dh = diff HEAD
	di = diff
	ff = merge --ff-only
	fp = format-patch
	fx = commit --fixup
	lg = log --graph --decorate --oneline
	lp = log --patch
	ls = ls-files --cached --abbrev
	rh = reset HEAD
	rl = reflog
	rp = reset --patch
	sa = status --short --branch
	sb = show-branch
	se = send-email
	sq = commit --squash
	st = status --short --branch -uno
	rbc	= rebase --continue
	top	= shortlog -ns
	egrep	= grep -E
	fgrep	= grep -F
[branch]
	autosetupmerge = always
	autosetuprebase = always
[color]
	ui = auto
[core]
	editor = vim
	pager = less -S
	excludesfile = ~/.gitignore
	attributesfile = ~/.gitattributes
[credential]
	helper = osxkeychain
[diff]
	renames = copies
	tool = vimdiff
[diff "gpg"]
	textconv = gpg2 -qd --no-tty
[diff "keyring"]
	textconv = gpg2 -v
	xfuncname = "^((pub|uid) .*)"
[diff "tar"]
	textconv = tar -tvf
[difftool]
	prompt = false
[grep]
	extendedregexp = true
[help]
	format = web
[init]
	templatedir = ~/.git_template
[instaweb]
	httpd = lighttpd -f
	local = true
	port = 8080
[interactive]
	singlekey = true
[log]
	decorate = short
[merge]
	conflictstyle = diff3
[pager]
	log = less -+$LESS -iRSq
[push]
	default = upstream
[rebase]
	autosquash = true
[rerere]
	enabled = true
[sendemail]
	smtpencryption = tls
	smtpserver = smtp.gmail.com
	smtpserverport = 587
	smtpuser = jacknagel@gmail.com
[user]
	email = jacknagel@gmail.com
	name = Jack Nagel
[web]
	browser = open
